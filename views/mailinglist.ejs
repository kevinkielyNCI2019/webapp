<% include ../partials/header.ejs %>

<!-- Page content -->
<div class="w3-content w3-padding" style="max-width:1564px">

  <!-- Project Section -->
  <div class="w3-container w3-padding-32" id="projects">
    <h3 class="w3-border-bottom w3-border-light-grey w3-padding-16">Mailing List</h3>
  </div>
  
 
 <div class="w3-row-padding w3-grayscale">
      
      <% jsonml.forEach(function(jsonml) { %>
      
    <div class="w3-col l3 m6 w3-margin-bottom">
      
      <h3>Mailing list Id: <%= jsonml.id %></h3>
      <p class="w3-opacity">Name: <%= jsonml.name %></p>
       <p>Email Address: <%= jsonml.emailadd %></p>
    
         <p><a href="/editmailinglist/<%= jsonml.id %>" class="w3-button w3-light-grey w3-block">Edit this entry</a></p>
         
         <p class="w3-button w3-light-grey w3-block" id="doit<%= jsonml.id %>">Delete this entry</a>
  <p id="yes<%= jsonml.id %>" style="display:none">
  Are you sure?
                        <a href="/deletemailinglist/<%= jsonml.id %>" class="w3-button w3-light-grey w3-block">Yes</a>
             </p>
             
             <script>
               $(document).ready(function(){
                 $("#doit<%= jsonml.id %>").click(function(){
                   $("#yes<%= jsonml.id %>").slideToggle(1000);
                   
                 })
                 
               })
               
             </script>
         
     
    </div>
    
    
    <% }); %>
    
  </div>
  
   <!-- <a href="/addcomment" class="w3-button w3-black">Add Comment</a> -->


</div>


<% include ../partials/footer.ejs %>